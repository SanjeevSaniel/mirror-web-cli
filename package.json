    "puppeteer",
    "framework-detection",
    "asset-optimization",
    "offline-website",
    "cli-tool",
    "web-development",
    "static-site-generator",
    "ai-powered",
    "openai",
    "website-analysis",
    "professional-tool"
  ],
  "author": {
    "name": "Sanjeev Saniel Kujur",
    "email": "sanjeevsaniel@gmail.com",
    "url": "https://github.com/SanjeevSaniel",
    "npm": "sanjeevsaniel"
  },
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=8.0.0"
  },
  "dependencies": {
    "axios": "^1.7.7",
    "chalk": "^5.5.0",
    "cheerio": "^1.1.2",
    "commander": "^14.0.0",
    "dotenv": "^16.4.5",
    "fs-extra": "^11.3.1",
    "jsdom": "^24.0.0",
    "node-fetch": "^3.3.2",
    "openai": "^4.67.3",
    "puppeteer": "^24.16.2",
    "sharp": "^0.33.5"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/SanjeevSaniel/mirror-web-cli.git"
